{
	"semi": false,
	"singleQuote": true,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"printWidth": 80,
	"endOfLine": "lf",
	"arrowParens": "always",
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"plugins": ["prettier-plugin-tailwindcss"],
	"htmlWhitespaceSensitivity": "ignore",
	"vueIndentScriptAndStyle": false
}
